<h1>Listing bettings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bettings.each do |betting| %>
      <tr>
        <td><%= link_to 'Show', betting %></td>
        <td><%= link_to 'Edit', edit_betting_path(betting) %></td>
        <td><%= link_to 'Destroy', betting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Betting', new_betting_path %>
